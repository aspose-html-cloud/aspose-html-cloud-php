{
    "description": 	"This repository contains Aspose_Html_Cloud_SDK for PHP source code. Aspose Cloud SDK for PHP lets PHP developers convert and process a variety of file formats in the cloud quickly and easily.",
    "name": 		"aspose/aspose-html-cloud-php",
    "type": 		"library",
    "keywords" : 	["html2pdf", "epub2pdf", "md2html", "html2md", "html2img", "svg2img", "svg2pdf","svg2xps", "html2xps", "epub2xps", "downloader", "seo", "validation", "convert],
	"license": 		"MIT",
    "homepage": "https://github.com/aspose-html-cloud/aspose-html-cloud-php",
    "authors": [
        {
            "name": "Aspose",
            "homepage": "https://github.com/aspose-html-cloud/aspose-html-cloud-php"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "aspose/storage-sdk-php": "*",
        "kamermans/guzzle-oauth2-subscriber": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.1",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": { "Client\\Invoker\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Client\\Invoker\\" : "test/" }
    },
     "version": "20.8.0"
}
